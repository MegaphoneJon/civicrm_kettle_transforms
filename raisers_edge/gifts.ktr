        <jsScript_script>var data = [];
var params = {};
// Line item vars are ignored on non-order APIs.
var lineItems = [];
var lineItemCount = 0;
var lineItemValues = {};
var price_field_id;
var price_field_value_id;
// Loop through the columns for this row.
for (var i=0;i&lt;getInputRowMeta().size();i++) {
  // Get the metadata for this column.
  var valueMeta = getInputRowMeta().getValueMeta(i);
  // Get the name of this column.
  var fieldName = valueMeta.getName();
  // Get the value of this column in this row.
  var fieldValue = valueMeta.getString(row[i]);
  // If this is a line_item param, add it to the lineItemValues object.
  if (fieldName.indexOf("line_item_") === 0) {
    // Get this value's field name.
    innerFieldName = String(fieldName).slice(10);
    // Split the values we previously concatenated back out.
    lineItemValues[innerFieldName] = String(fieldValue).split(", ");
    lineItemCount = lineItemValues[innerFieldName].length;
  }
  // Otherwise, just add to params (unless it's blank).
  else if (fieldValue) {
    params[fieldName] = String(fieldValue); //valueMeta.getString(row[i]);
  }
}

// OK, at this point we've added all the non-line item params and put the line item params in an array.
// Now, let's build the line items array.
if (lineItemCount) {
  for (var j = 0; j &lt; lineItemCount; j++) {
    // Build the ridiculous data structure.
    var lineItem = {};
    lineItem.line_item = {};
    lineItem.line_item[j] = {};
    // Loop through the lineItemValues object and populate the line items.
    for (var innerFieldName in lineItemValues) {
      lineItem.line_item[j][innerFieldName] = lineItemValues[innerFieldName][j];
    }
    // Add in some required fields for line items.
    lineItem.line_item[j].entity_table = 'civicrm_contribution';
    lineItem.line_item[j].qty = 1;
    lineItem.line_item[j].price_field_id = price_field_id;
    // We've generated several PriceFieldValue records (10 to start), sequentially.
    lineItem.line_item[j].price_field_value_id = price_field_value_id + j;
    lineItem.line_item[j].unit_price = lineItem.line_item[j].line_total;
    lineItems.push(lineItem);
  }

  // Put the line items into params.
  params.line_items = lineItems;
}


var paramsJson = JSON.stringify(params);
data =  '[[ "Order", "create", ' + paramsJson + ']]';
var temp = JSON.stringify(lineItemValues);</jsScript_script>
