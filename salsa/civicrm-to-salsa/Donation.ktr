    <connection>Target CiviCRM</connection>
    <sql>SELECT
 CONCAT('CONT-', cc.id) AS id
, cc.contact_id
, payment_instrument_id
, receive_date
, non_deductible_amount
, total_amount
, fee_amount
, net_amount
, trxn_id
, invoice_id
, currency
, cancel_date
, cancel_reason
, receipt_date
, thankyou_date
, cc.source
, amount_level
, contribution_recur_id
, is_test
, is_pay_later
, contribution_status_id
, address_id
, check_number
, campaign_id
, tax_amount
, creditnote_id
, revenue_recognition_date
, cft.name AS financial_type
, campaign.title AS campaign
FROM civicrm_contribution cc
JOIN civicrm_financial_type cft ON cc.financial_type_id = cft.id
LEFT JOIN civicrm_campaign campaign ON cc.campaign_id = campaign.id
JOIN civicrm_contact contact ON cc.contact_id = contact.id
WHERE contact.is_deleted &lt;&gt; 1</sql>
