    <connection>Raiser's Edge</connection>
    <sql>SELECT
  ADDRESS_BLOCK
, CITY
, POST_CODE
, "STATE"
, CTY.LONGDESCRIPTION as country
, ADDRESS_ID
, CONSTIT_ID
, PREFERRED
, LOC_TYPE.LONGDESCRIPTION
, r.CONSTITUENT_ID
FROM ADDRESS
LEFT JOIN TABLEENTRIES AS CTY ON CTY.TABLEENTRIESID = COUNTRY
JOIN CONSTIT_ADDRESS ON ADDRESS.ID = CONSTIT_ADDRESS.ADDRESS_ID
LEFT JOIN TABLEENTRIES AS LOC_TYPE ON CONSTIT_ADDRESS."TYPE" = LOC_TYPE.TABLEENTRIESID
LEFT JOIN RECORDS r ON CONSTIT_ADDRESS.CONSTIT_ID = r.ID
WHERE INDICATOR &lt;&gt; 7 AND INDICATOR &lt;&gt; 1 AND ADDRESS_BLOCK IS NOT NULL</sql>
