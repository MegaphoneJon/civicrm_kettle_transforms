        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// We're going to assume these defaults haven't been changed in Civi:
//| 1     | Phone       |
//| 2     | Email       |
//| 3     | Postal Mail |
//| 4     | SMS         |
//| 5     | Fax         |
var preferred_contact_method = null;

// If they've opted out of phone/email/mail, set to "Do Not Contact".
// Otherwise, parse their preferred_communication_method.
// Since preferred_communication_method is multi-value but preferred_contact_method isn't,
// I've made some value judgments about which ones take precedence (lower = higher precedence)
if (preferred_communication_method) {
  if (preferred_communication_method.indexOf('3') !== -1) {
    preferred_contact_method = "Standard Mail";
  }
  if (preferred_communication_method.indexOf('1') !== -1) {
    preferred_contact_method = "Phone";
  }
  if (preferred_communication_method.indexOf('2') !== -1) {
    preferred_contact_method = "Email";
  }
}

if (do_not_mail == 1 &amp;&amp; do_not_phone == 1 &amp;&amp; (do_not_email == 1 || is_opt_out == 1)) {
  preferred_contact_method = "Do not contact";
}</jsScript_script>
