    <sql>SELECT
  dp.id AS donation_pages_id
, dp.page_id
, dp.tag_list
, dp.created_at AS created_date
, dp.maximum_amount_in_cents AS max_amount
, dp.minimum_amount_in_cents AS min_amount
, dp.amount_options
, dp.merchant_account_id
, dp.donation_tracking_code_id
, dp.amount_goal_in_cents AS goal_amount
, dp.content_html AS intro_text
, p.headline AS title
, np.headline AS thankyou_title
, bp.content_html AS thankyou_text
FROM ${SCHEMANAME}.donation_pages dp
JOIN ${SCHEMANAME}.pages p ON dp.page_id = p.id
-- np is the "thank-you" page
LEFT JOIN ${SCHEMANAME}.pages np ON dp.next_page_id = np.id
LEFT JOIN ${SCHEMANAME}.basic_pages bp ON np.id = bp.page_id</sql>
