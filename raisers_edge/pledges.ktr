  <step>
    <name>Calculate frequency_day</name>
    <type>ScriptValueMod</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//What day of the period is the payment due?
var frequency_day;
temp_date = null;
first_of_year = null;


// Annual.  This is the hardest; need to calculate day of year of, e.g. November 3rd.
if (INSTALLMENT_FREQUENCY == 1) {
  // "2017" can be any non-leap year
  temp_date = new Date(2017, Schedule_Month - 1, Schedule_DayOfMonth);
  start = new Date(2017, 0, 0);
  diff = temp_date - start + (start.getTimezoneOffset() - temp_date.getTimezoneOffset()) * 60 * 1000;
  //var diff = now - start;
  oneDay = 1000 * 60 * 60 * 24;
  frequency_day = Math.floor(diff / oneDay);
}

// Every 1/2/3/6 months
if (INSTALLMENT_FREQUENCY >= 2 &amp;&amp; INSTALLMENT_FREQUENCY &lt;= 5) {
  // "X Day of the Month"-style scheduling
  if (Schedule_MonthlyType == 1) {
    frequency_day = Schedule_DayOfMonth;
  }
  // "3rd Friday of the Month"-style scheduling.  Can't really do this in Civi, so make it up.
  if (Schedule_MonthlyType == 2) {
    frequency_day = 30;
  }
}

// Twice a month on arbitrary days.  Civi doesn't support it and I don't really need to either, so...
if (INSTALLMENT_FREQUENCY == 6) {
  frequency_day = 1;
}

// Irregular - can't really say.
if (INSTALLMENT_FREQUENCY == 9) {
  frequency_day = 1;
}

// Single Installment
if (INSTALLMENT_FREQUENCY == 10) {
  frequency_day = 1;
}

</jsScript_script>
      </jsScript>
    </jsScripts>
