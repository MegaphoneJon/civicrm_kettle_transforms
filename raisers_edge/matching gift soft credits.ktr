    <connection>Raiser's Edge</connection>
    <sql>/* Find all pledge installments, and their related payments if they exist. */
SELECT
i.InstallmentId
, i.PledgeId
, i.AdjustmentId
, i.Amount as scheduled_amount
, i.Dte
, ip.Amount as actual_amount
, ip.PaymentId
, original_gift.CONSTIT_ID
, g.RECEIPT_AMOUNT
, g.DTE as receive_date
, g.TYPE
, DBO.TranslateGiftType(g.TYPE) as type_1
/* Get the original pledge type too, for filtering */
, original_pledge.TYPE
, DBO.TranslateGiftType(original_pledge.TYPE) as original_pledge_type
FROM Installment i
JOIN InstallmentPayment ip ON i.InstallmentId = ip.InstallmentId
LEFT JOIN GIFT g ON ip.PaymentId = g.ID
LEFT JOIN GIFT original_pledge ON i.PledgeId = original_pledge.ID
LEFT JOIN GiftMatchingGift gmg ON i.PledgeId = gmg.MatchingGiftId
LEFT JOIN GIFT original_gift ON gmg.GiftId = original_gift.ID
/* Adjustments are stored in here too - when an adjustment happens, the pledge ID of the original value is blanked */
WHERE i.PledgeId IS NOT NULL
ORDER BY i.AdjustmentId
/* Write-off Types: Covenant WriteOff, MG Write Off, Write Off */</sql>
