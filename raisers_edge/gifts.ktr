        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Ugh, RE hard-codes Pay Method to 1980's pay methods - so people use Gift Type.
var payment_instrument;
var type;
// Pledges are already filtered out.

// These are duplicative
if (type == 'Pay-Cash' || type == 'MG Pay-Cash') {
  type = 'Cash';
}
if (type == 'Pay-Stock/Property' || type == 'Stock/Property') {
  type = 'Stock';
}
if (type == 'Pay-Gift-in-Kind') {
  type = 'Gift-in-Kind';
}
if (type == 'Pay-Other') {
  type = 'Other';
}

//If type is 'Cash', the Payment Type is final.
if (type == 'Cash') {
}

//These gift types always trump Pay Method.
if (type == 'Stock' || type == 'Gift-in-Kind') {
  payment_instrument = type;
}</jsScript_script>
