    <sql>SELECT
cs.CONSTIT_ID,
cs.SALUTATION_ID,
LONGDESCRIPTION as SAL_TYPE,
cs.SEQUENCE,
cs.SALUTATION,
cs.EDITABLE
FROM CONSTITUENT_SALUTATION cs
JOIN TABLEENTRIES te ON te.TABLEENTRIESID = cs.SAL_TYPE

UNION ALL
SELECT
ID as CONSTIT_ID,
PRIMARY_ADDRESSEE_ID as SALUTATION_ID,
'Addressee' as SAL_TYPE,
-2 as SEQUENCE,
PRIMARY_ADDRESSEE as SALUTATION,
PRIMARY_ADDRESSEE_EDIT as EDITABLE
FROM RECORDS
WHERE KEY_INDICATOR = 'I' AND PRIMARY_ADDRESSEE_ID IS NOT NULL

UNION ALL
SELECT
ID as CONSTIT_ID,
PRIMARY_SALUTATION_ID as SALUTATION_ID,
'Postal' as SAL_TYPE,
-1 as SEQUENCE,
PRIMARY_SALUTATION as SALUTATION,
PRIMARY_SALUTATION_EDIT as EDITABLE
FROM RECORDS
WHERE KEY_INDICATOR = 'I' AND PRIMARY_SALUTATION_ID IS NOT NULL</sql>
