    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var format;
var current_line;
// Check to see if we've moved to the next country, in which case start over.
if (SEQUENCE == 1) {
  current_country = country;
  format = '{contact.addressee}\n';
  current_line = LINE_NUM;
}
// Add newlines where applicable
if (LINE_NUM > current_line) {
  format = format + "\n";
  current_line = LINE_NUM;
}
// If the token is "address lines", put in all the street lines from Civi.
if (token == 'address_lines') {
  token = "{contact.street_address}\n{contact.supplemental_address_1}\n{contact.supplemental_address_2}\n{contact.supplemental_address_3}";
}
format = format + token;

</jsScript_script>
      </jsScript>
    </jsScripts>
