    <connection>Raiser's Edge</connection>
    <sql>SELECT t.LONGDESCRIPTION as country
, cl.FIELD_NUM
, cl.LINE_NUM
, cl.COUNTRY_ID
, cl.SEQUENCE
FROM COUNTRY_LABELS cl
JOIN COUNTRY_CODES cc ON cl.COUNTRY_ID = cc.ID
JOIN TABLEENTRIES t ON cc.COUNTRYCODESID = t.TABLEENTRIESID 
ORDER BY cl.COUNTRY_ID, cl.SEQUENCE</sql>
    <limit>0</limit>
    <lookup />
    <execute_each_row>N</execute_each_row>
