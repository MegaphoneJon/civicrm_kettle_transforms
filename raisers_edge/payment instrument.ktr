  <step>
    <name>RE: Gift subtypes in use</name>
    <type>TableInput</type>
    <description />
    <distribute>N</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Raiser's Edge</connection>
    <sql>/* We only bring in gift subtypes that exist on a gift. */
SELECT g.GIFTSUBTYPE
FROM GiftSplit gs
LEFT JOIN FUND on gs.FundId = FUND.id 
LEFT JOIN GIFT g on gs.GiftId = g.ID
LEFT JOIN TABLEENTRIES gst on g.GIFTSUBTYPE = gst.TABLEENTRIESID
WHERE gst.LONGDESCRIPTION IS NOT NULL</sql>
    <limit>0</limit>
    <lookup />
    <execute_each_row>N</execute_each_row>
