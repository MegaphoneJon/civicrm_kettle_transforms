    <sql>SELECT ccf.label
, ccf.name
, ccg.name as custom_group_id
, cog.name as option_group
, ccf.data_type
, ccf.html_type
, ccf.default_value
, ccf.is_required
, ccf.is_searchable
, ccf.is_search_range
, ccf.weight
, ccf.is_active
, ccf.is_view
, ccf.text_length
, ccf.in_selector
FROM civicrm_custom_field ccf
JOIN civicrm_custom_group ccg ON ccf.custom_group_id = ccg.id
LEFT JOIN civicrm_option_group cog ON ccf.option_group_id = cog.id
ORDER BY ccf.id
</sql>
