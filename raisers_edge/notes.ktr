        <jsScript_script>//Script here

//Script here

var ActualNotes;
var Author;
var NoteType;
var Description;

if (ActualNotes == null){
  ActualNotes = '';
}

if (Description != null){
	Description = 'Description: ' + Description + '\n';
	ActualNotes = Description + ActualNotes;
}

if (Author != null){
	Author = 'Author: ' + Author + '\n';
	ActualNotes = Author + ActualNotes;
}

if (NoteType != null){
	NoteType = 'Note Type: ' + NoteType + '\n';
	ActualNotes = NoteType + ActualNotes;
}</jsScript_script>
