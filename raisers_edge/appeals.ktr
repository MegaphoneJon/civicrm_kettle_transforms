    <connection>Raiser's Edge</connection>
    <sql>/* We only bring in appeals that exist on a gift or a constituent appeal. */
SELECT
g.CONSTIT_ID
, APPEAL.DESCRIPTION as appeal
, r.CONSTITUENT_ID
FROM GiftSplit gs
LEFT JOIN Appeal on gs.AppealId = APPEAL.id 
LEFT JOIN GIFT g on gs.GiftId = g.ID
LEFT JOIN RECORDS r ON g.CONSTIT_ID = r.ID
WHERE APPEAL.DESCRIPTION IS NOT NULL
UNION
SELECT
ca.CONSTIT_ID
, APPEAL.DESCRIPTION as appeal
, r.CONSTITUENT_ID
FROM CONSTITUENT_APPEALS ca
LEFT JOIN APPEAL on ca.APPEAL_ID = APPEAL.id
JOIN RECORDS r ON ca.CONSTIT_ID = r.ID
WHERE APPEAL.DESCRIPTION IS NOT NULL</sql>
