    <connection>Raiser's Edge</connection>
    <sql>SELECT
f.ID
, f.DESCRIPTION
, f.FUND_CATEGORY
, f.FUND_ID
, f.NOTES
, f.RESTRICTED_FLAG
, f."SEQUENCE"
, f.START_DATE
, f.END_DATE
, f.FUNDTYPE
, gldd.GLDEBIT_NUMBER
, gldd.GLCREDIT_NUMBER
-- Here is a client-specific bit
, t.LONGDESCRIPTION as gift_type
, t2.LONGDESCRIPTION as gift_subtype
FROM FUND f
LEFT JOIN FundDistribution fd ON f.ID = fd.FundId
LEFT JOIN GLDISTRIBUTIONDETAIL gldd on fd.GLDistributionID = gldd.DISTRIBUTION_ID
LEFT JOIN TABLEENTRIES t ON fd.GiftTypeID = t.TABLEENTRIESID
LEFT JOIN TABLEENTRIES t2 ON fd.GiftSubTypeID = t2.TABLEENTRIESID
ORDER BY f.ID

</sql>
