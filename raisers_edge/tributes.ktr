    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var soft_credit_type_id;
var pcp_personal_note;

// Handle null values.
if (!soft_credit_type_id) {
  pcp_personal_note = 'Migrated from Raisers Edge with no tribute type: ' + pcp_personal_note;
}
if (soft_credit_type_id) {
switch (String(soft_credit_type_id.toString())) {
  case 'in honor of Mr. and Mrs.':
    pcp_personal_note = 'Mr. and Mrs.: ' + pcp_personal_note;
    break;
  case 'in honor of the anniversary of':
  case 'in honor of wedding anniversary':
    pcp_personal_note = 'Anniversary: ' + pcp_personal_note;
    break;
  case 'in honor of the birth of':
    pcp_personal_note = 'Birth: ' + pcp_personal_note;
    break;
  case 'in honor of the birthday':
  case 'in lieu of birthday gift to':
    pcp_personal_note = 'Birthday: ' + pcp_personal_note;
    break;
  case 'in honor of the christening of':
    pcp_personal_note = 'Christening: ' + pcp_personal_note;
    break;
  case 'in honor of the graduation of':
    pcp_personal_note = 'Graduation: ' + pcp_personal_note;
    break;
  case 'in honor of Wedding of':
  case 'in lieu of Wedding gifts':
    pcp_personal_note = 'Wedding: ' + pcp_personal_note;
    break;
  case 'in lieu of a gift':
  case 'in lieu of Christmas Gift to':
    pcp_personal_note = 'in lieu of gift: ' + pcp_personal_note;
    break;
  case 'in lieu of flowers':
    pcp_personal_note = 'in lieu of flowers: ' + pcp_personal_note;
    break;
  case 'c/o':
    pcp_personal_note = 'c/o: ' + pcp_personal_note;
    break;
}
