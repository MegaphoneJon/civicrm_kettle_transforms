    <connection>Raiser's Edge</connection>
    <sql>SELECT
gs.GiftId
, g.CONSTIT_ID
, gs.Amount
, g.RECEIPT_AMOUNT
, g.DTE as receive_date
, gs.FundId
, f.DESCRIPTION as fund_name
, gs.CampaignId
, gs.AppealId
, gs.PackageId
, g.PAYMENT_TYPE
, g.ACKNOWLEDGEDATE
, g.CHECK_NUMBER
, g.CHECK_DATE
, g.BATCH_NUMBER
, g.ANONYMOUS
, g.GIFTSUBTYPE as giftsubtype
, g.TYPE as type_id
, DBO.TranslateGiftType(g.TYPE) as type
, g.REF as note
, g.REFERENCE_DATE as note_date
-- , BOOLEAN as recurring_gift
, r.CONSTITUENT_ID
FROM GiftSplit gs
LEFT JOIN GIFT g on gs.GiftId = g.ID
-- LEFT JOIN GiftAttributes ON GiftID = PARENTID
LEFT JOIN RECORDS r ON g.CONSTIT_ID = r.ID
JOIN FUND f ON gs.FundId = f.ID</sql>
